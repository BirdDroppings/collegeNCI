<? xml version="1.0"?>
<products xlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="products.xsd">
<xs:element name="products">
<xs:complexType>
<xs:element name="Product"/>
<xs:element name="Category" type="xsd:String"/>
<xs:element name="code" type="xsd:integer"/>
<xs:element name="name" type="xsd:String"/>
<xs:element name="description" type="xsd:String"/>
<xs:element name="quantity" type="xsd:double"/>
<xs:element name="unitPrice" type="xsd:integer"/>
</xs:Product>
</xs:complexType >
</xs:element>


</xs:schema>